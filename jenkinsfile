node{
    stage('Build'){
        echo 'building a project'
    }
    stage('Verify Dotnet') {
    steps {
        bat 'dotnet --info'
    }
    }
    stage('test'){
        echo 'testing a project'
    }
    stage('deploy'){
        echo 'release a project'
    }
    stage('release'){
        echo 'release a project'
    }
}
